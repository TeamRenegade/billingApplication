@model VisitorDemo.Models.HeaderVM


<div id="header" class="row">

    <div class="col-md-10">
        <h4 class="text-left">@Model.Name</h4>
    </div>
    <div class="row">
        <div  class="col-md-offset-0 col-md-7">
            <small class="text-left">@Model.Address1,@Model.Address2,@Model.City,@Model.Country,@Model.Zip</small>

        </div>
        <div class="col-md-3">
            <small class="text-right">Email:@Model.Email,Website:@Model.Website</small>

        </div>
       
    </div>
    
    
</div>
<hr/>